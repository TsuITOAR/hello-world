{
	// 使用 IntelliSense 了解相关属性。 
	// 悬停以查看现有属性的描述。
	// 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			//cpp LeetCode
			"name": "Cpp",
			"preLaunchTask": "LeetCode cppbuild",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${fileDirname}\\..\\bin\\Main.exe",
			"symbolSearchPath": "https://msdl.microsoft.com/download/symbols",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "externalTerminal",
		},
		{
			//rust LeetCode
			"name": "Rust",
			"preLaunchTask": "build",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/LeetCode_rust/target/debug/rust_main.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}\\LeetCode_Rust",
			"environment": [],
			"console": "externalTerminal",
		},
		{
			//rust LeetCode
			"name": "Rust with back trace",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "cargo",
			"args": [
				"run"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}\\LeetCode_Rust",
			"environment": [
				{
					"name": "RUST_BACKTRACE",
					"value": "1"
				}
			],
			"console": "integratedTerminal",
		},
		{
			"name": "Rust (GDB)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/LeetCode_Rust/target/debug/rust_main.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}\\LeetCode_Rust",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "C:\\Program Files\\mingw-w64\\x86_64-8.1.0-posix-seh-rt_v6-rev0\\mingw64\\bin\\gdb.exe",
			"setupCommands": [
				{
					"description": "为 gdb 启用整齐打印",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}