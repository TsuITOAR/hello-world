{
	"env": {
		"Platform": "x64",
		"OS": "windows",
		"myIncludePath": [
			"${env:OneDrive}\\Documents\\Visual Studio Code\\MyInclude\\"
		],
		"vcpkgInclude": "${env:vcpkgpath}\\installed\\${Platform}-${OS}\\include\\",
		"MsvcCompilerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2019/BuildTools/VC/Tools/MSVC/14.28.29333/bin/Host${Platform}/${Platform}/cl.exe",
		"GccCompilerPath": "C:/Program Files/mingw-w64/x86_64-8.1.0-posix-seh-rt_v6-rev0/mingw64/bin/g++.exe"
	},
	"configurations": [
		{
			"name": "Msvc",
			"includePath": [
				"${workspaceFolder}/**",
				"${vcpkgInclude}",
				"${myIncludePath}"
			],
			"browse": {
				"limitSymbolsToIncludedHeaders": true
			},
			"defines": [
				//\MDd
				"_DLL",
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"windowsSdkVersion": "10.0.18362.0",
			"compilerPath": "${MsvcCompilerPath}",
			"cStandard": "c17",
			"cppStandard": "c++20",
			"intelliSenseMode": "msvc-x64"
			//"configurationProvider": "ms-vscode.cmake-tools"
		},
		{
			"name": "Gcc",
			"includePath": [
				"${workspaceFolder}/**",
				"${vcpkgInclude}",
				"${myIncludePath}"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"windowsSdkVersion": "10.0.18362.0",
			"compilerPath": "${GccCompilerPath}",
			"cStandard": "c17",
			"cppStandard": "c++20",
			"intelliSenseMode": "gcc-x64"
		}
	],
	"version": 4
}